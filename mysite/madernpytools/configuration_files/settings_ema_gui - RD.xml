<EMA_GUI_settings>
	<n_reps>3</n_reps>
	<dir>./</dir>
	<Modal_Settings>
		<DAQ_Settings>
			<task_name>ModalTest</task_name>
			<chassis_name>MadernRD</chassis_name>
			<sampling_rate>25600</sampling_rate>
		</DAQ_Settings>
		<sensor_settings>
			<sensor name="Acc1">
				<name>Acc1</name>
				<device_name>MadernRDMod1</device_name>
				<channel>0</channel>
				<type>acceleration</type>
				<unit>m/s2</unit>
			</sensor>
			<sensor name="Acc2">
				<name>Acc2</name>
				<device_name>MadernRDMod1</device_name>
				<channel>1</channel>
				<type>acceleration</type>
				<unit>m/s2</unit>
			</sensor>
			<sensor name="impact hammer">
				<name>impact hammer</name>
				<device_name>MadernRDMod1</device_name>
				<channel>2</channel>
				<type>impact_hammer</type>
				<unit>N</unit>
			</sensor>
		</sensor_settings>
		<log_settings>
			<test_description>Modal test</test_description>
			<impact_trigger>20.0</impact_trigger>
			<impact_timeout>30</impact_timeout>
			<test_timeout>30</test_timeout>
			<log_size>51200</log_size>
		</log_settings>
	</Modal_Settings>
	<spectral_settings>
		<window_fraction>0.5</window_fraction>
		<overlap_fraction>0.5</overlap_fraction>
		<i_input>3</i_input>
		<i_output>1</i_output>
		<f_range>[0, 5000]</f_range>
		<filter_window>('exponential', None, 1000000.0)</filter_window>
		<scaling>density</scaling>
		<sampling_rate>25600</sampling_rate>
	</spectral_settings>
</EMA_GUI_settings>